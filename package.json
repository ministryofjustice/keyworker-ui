{
  "name": "keyworker-ui",
  "description": "Nodejs application to support Key worker related tasks.",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=3",
    "node": ">=8.4.*"
  },
  "dependencies": {
    "applicationinsights": "^1.0.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-healthcheck": "^0.1.0",
    "govuk-elements-sass": "^3.1.2",
    "helmet": "^3.12.0",
    "hsts": "^2.1.0",
    "immutable": "^3.8.2",
    "jsonwebtoken": "^8.1.1",
    "react": "^16.2.0",
    "react-appinsights": "^1.0.4",
    "react-datetime": "^2.12.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-tooltip": "^3.4.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start-frontend": "node scripts/customized-config start | bunyan -o short",
    "start-backend": "node backend/index | bunyan -o short",
    "start-live": "concurrently --kill-others-on-fail -p \"[{name}]\" -n \"SERVER,CLIENT\" -c \"bgCyan.bold,bgGrey.bold\" \"yarn start-backend\" \"yarn start-frontend\"",
    "start": "node server",
    "build": "node scripts/customized-config build",
    "test": "node scripts/customized-config test --env=jsdom",
    "lint": "node node_modules/eslint/bin/eslint.js src backend",
    "verify": "node scripts/customized-config test --coverage && yarn lint",
    "record-build-info": "node scripts/record-build-info",
    "plant-beanstalk": "node scripts/plant-beanstalk.js"
  },
  "proxy": {
    "/auth/*": {
      "target": "http://localhost:3001"
    },
    "/api/*": {
      "target": "http://localhost:3001"
    },
    "/health": {
      "target": "http://localhost:3001"
    },
    "/info": {
      "target": "http://localhost:3001"
    }
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.6.0",
    "mocha": "^5.0.0",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "proxyquire": "^1.8.0",
    "rewire": "^3.0.2",
    "sass-loader": "^6.0.6"
  }
}
